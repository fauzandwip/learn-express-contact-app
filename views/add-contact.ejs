<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h2>Form Add Contact</h2>
			<form method="post" action="/contacts">
				<div class="mb-3">
					<label for="name" class="form-label">Name</label>
					<input
						type="text"
						class="form-control"
						id="name"
						name="name"
						autocomplete="off"
						required
					/>
				</div>
				<div class="mb-3">
					<label for="email" class="form-label">Email</label>
					<input
						type="email"
						class="form-control"
						id="email"
						name="email"
						autocomplete="off"
					/>
				</div>
				<div class="mb-3">
					<label for="name" class="form-label">Number Phone</label>
					<input
						type="text"
						class="form-control"
						id="name"
						name="noHP"
						autocomplete="off"
						required
					/>
				</div>
				<button type="submit" class="btn btn-primary">Add Contact</button>
			</form>
		</div>
	</div>
</div>
